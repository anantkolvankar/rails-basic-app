<div class="panel panel-default" id="panel_<%= item.id %>" >
<div class="panel-heading">
  <h4 class="panel-title">
    <a data-toggle="collapse" data-parent="#item_acc" href="#collapse_<%= item.id %>" class="nounderline" data-parent="#item-acc"><%= item.name %></a>
  </h4>
</div>
<div id="collapse_<%= item.id %>" class="panel-collapse collapse">
  <div class="panel-body">Panel Body <%= item.id %></div>
  <div class="panel-footer">Done <%= check_box_tag 'release[release_item_attributes][item_ids][]', item.id, nil, :class=> 'item'%></div>
</div>
</div>